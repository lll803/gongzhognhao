# Supabase 配置
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# TopHub API 配置
TOPHUB_API_KEY=your_tophub_api_key
TOPHUB_BASE_URL=https://api.tophubdata.com

# MongoDB 配置（现有系统）
MONGODB_URI=mongodb://localhost:27017/gongzhonghao-cms

# OpenAI 配置（现有系统）
OPENAI_API_KEY=your_openai_api_key
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.7

# OpenRouter API 配置（AI改写功能）
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_API_BASE=https://openrouter.ai/api/v1
OPENROUTER_MODEL=openai/gpt-3.5-turbo
OPENROUTER_MAX_TOKENS=2000
OPENROUTER_TEMPERATURE=0.7

# JWT 配置（现有系统）
JWT_SECRET=your_jwt_secret_key

# FAL 文生图（可选，若未使用火山引擎）
FAL_KEY=your_fal_api_key
FAL_MODEL_ENDPOINT=https://fal.run/fal-ai/flux/schnell
FAL_GUIDANCE_SCALE=3.5
FAL_NUM_INFERENCE_STEPS=28
FAL_WIDTH=896
FAL_HEIGHT=504

# 火山引擎 Ark 文生图（已移除，可忽略）